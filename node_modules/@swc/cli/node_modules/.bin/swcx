#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/@swc+cli@0.1.62_@swc+core@1.3.62/node_modules/@swc/cli/bin/node_modules:/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/@swc+cli@0.1.62_@swc+core@1.3.62/node_modules/@swc/cli/node_modules:/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/@swc+cli@0.1.62_@swc+core@1.3.62/node_modules/@swc/node_modules:/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/@swc+cli@0.1.62_@swc+core@1.3.62/node_modules:/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/@swc+cli@0.1.62_@swc+core@1.3.62/node_modules/@swc/cli/bin/node_modules:/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/@swc+cli@0.1.62_@swc+core@1.3.62/node_modules/@swc/cli/node_modules:/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/@swc+cli@0.1.62_@swc+core@1.3.62/node_modules/@swc/node_modules:/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/@swc+cli@0.1.62_@swc+core@1.3.62/node_modules:/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/swcx.js" "$@"
else
  exec node  "$basedir/../../bin/swcx.js" "$@"
fi
