#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/webpack-cli@5.1.3_webpack@5.85.1/node_modules/webpack-cli/bin/node_modules:/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/webpack-cli@5.1.3_webpack@5.85.1/node_modules/webpack-cli/node_modules:/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/webpack-cli@5.1.3_webpack@5.85.1/node_modules:/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/webpack-cli@5.1.3_webpack@5.85.1/node_modules/webpack-cli/bin/node_modules:/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/webpack-cli@5.1.3_webpack@5.85.1/node_modules/webpack-cli/node_modules:/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/webpack-cli@5.1.3_webpack@5.85.1/node_modules:/mnt/f/微前端学习/micro-frontends-plugin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-cli/bin/cli.js" "$@"
else
  exec node  "$basedir/../webpack-cli/bin/cli.js" "$@"
fi
